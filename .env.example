# PhotoChanger default configuration for local development
PHOTOCHANGER_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/photochanger
PHOTOCHANGER_QUEUE_STATEMENT_TIMEOUT_MS=5000
PHOTOCHANGER_QUEUE_MAX_IN_FLIGHT_JOBS=12
PHOTOCHANGER_STATS_DATABASE_URL=
PHOTOCHANGER_STATS_SLOT_CACHE_TTL_SECONDS=300
PHOTOCHANGER_STATS_GLOBAL_CACHE_TTL_SECONDS=60
PHOTOCHANGER_STATS_RECENT_RESULTS_RETENTION_HOURS=72
PHOTOCHANGER_STATS_RECENT_RESULTS_LIMIT=10
PHOTOCHANGER_WORKER_POLL_INTERVAL_MS=1000
PHOTOCHANGER_WORKER_MAX_POLL_ATTEMPTS=10
PHOTOCHANGER_WORKER_RETRY_ATTEMPTS=5
PHOTOCHANGER_WORKER_RETRY_BACKOFF_SECONDS=3.0
PHOTOCHANGER_WORKER_REQUEST_TIMEOUT_SECONDS=5.0
PHOTOCHANGER_JWT_SECRET=change-me
PHOTOCHANGER_JWT_ACCESS_TTL_SECONDS=900
PHOTOCHANGER_ADMIN_CREDENTIALS_PATH=secrets/runtime_credentials.json
