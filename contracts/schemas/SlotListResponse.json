{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "SlotListResponse",
  "type": "object",
  "required": ["data", "meta"],
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "$ref": "./Slot.json"
      }
    },
    "meta": {
      "type": "object",
      "required": ["total"],
      "properties": {
        "total": {
          "type": "integer",
          "minimum": 0,
          "description": "Количество доступных слотов"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
